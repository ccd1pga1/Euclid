\newcommand{\ETri}[4]{%
	\tkzDefPoint(#1){#2}%
 \tkzDefShiftPoint[A](60:3){#3}%<------------45 for right angles iso
 \tkzDefShiftPoint[A](0:3){#4}%
 \tkzDrawPolygon(#2,#3,#4)%
 \tkzDrawPoints(#2,#3,#4)%
 \tkzLabelPoints[below](#2,#4)%
 \tkzLabelPoints[above](#3)%
}%

%%%%New command to make triangle based on shifting the first point by(ang:dist)
\newcommand{\TRI}[7]{%
	\tkzDefPoint(#1){#2}%
 \tkzDefShiftPoint[#2](#3){#4}%
 \tkzDefShiftPoint[#2](#5){#6}%
 \tkzDrawPolygon[fill=#7](#2,#4,#6)%
 \tkzDrawPoints(#2,#4,#6)%
 \tkzLabelPoints[below](#2,#4)%
 \tkzLabelPoints[above](#6)%
}%

\newcommand{\AB}[1]{%
	\tkzDefPoint(0,0){A}%
	\tkzDefPoint(3,0){B}%
}%

\newcommand{\A}[1]{%
	\tkzDefPoint(0,0){A}%
}%

\newcommand{\B}[1]{%
	\tkzDefPoint(3,0){B}%	
}%

\newcommand{\C}[1]{%
	\tkzDefPoint(1.5,2.5){C}%
}%

\newcommand{\Lem}[2]{%
  \begin{lemma}%
    {#1}%
    \ifx&#2&%
    % No items provided, do nothing
    \else
      \begin{itemize}%
        #2%
      \end{itemize}%
    \fi
  \end{lemma}%
}

\newcommand{\Tri}[8]{%%
	\tkzDefPoint(0,0){#1}%
	\tkzDefPoint({#2,#3}){#4}%
	\tkzDefPoint({#5,#6}){#7}%
	\tkzDrawPolygon[fill=#8](#1,#4,#7)%
	\tkzLabelPoint[left](#1){$#1$}% 
	\tkzLabelPoint[below](#4){$#4$}%
	\tkzLabelPoint[above](#7){$#7$}%
	\tkzDrawPoints(#1,#4,#7)%
}

\newcommand{\iTri}[4]{%
	\tkzDefPoint(0,0){#1}%
	\tkzDefPoint(3,0){#2}%
	\tkzDefPoint(1.5,3){#3}%
	\tkzDrawPolygon[fill=#4, opacity=0.3](#1,#2,#3)% 
	\tkzLabelPoint[left](#1){$#1$}% 
	\tkzLabelPoint[right](#2){$#2$}%
	\tkzLabelPoint[above](#3){$#3$}%
	\tkzMarkSegment[mark=|](#1,#3)%
	\tkzMarkSegment[mark=|](#2,#3)%
}	
	
\newcommand{\tri}[7]{%
\tkzDefPoint(0,0){#1}% 
\tkzDefPoint(#2,#3){#4}%
 \tkzDefPointBy[rotation=center #1 angle #5](#4)%
